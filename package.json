    "ava": "^0.16.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.3.13",
    "chalk": "^1.1.3",
    "mkdirp": "^0.5.1",
    "nyc": "^9.0.1",
    "semistandard": "^9.0.0",
    "snazzy": "^5.0.0"
